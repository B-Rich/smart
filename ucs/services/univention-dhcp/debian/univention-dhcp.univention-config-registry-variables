[dhcpd/ldap/base]
Description[de]=LDAP-Basis für den DHCP-Dienst
Description[en]=LDAP base for the DHCP service
Type=str
Categories=service-dhcp

[dhcpd/authoritative]
Description[de]=Definiert, ob der DHCP-Dienst maßgeblich ist und DHCP-NAK-Nachrichten an falsch konfigurierte Rechner sendet [false]
Description[en]=Defines if the DHCP service is authoritative and sends DHCP-NAK messages to wrongly configured clients [false]
Type=bool
Categories=service-dhcp

[dhcpd/autostart]
Description[de]=Definiert, ob der DHCP-Dienst automatisch beim Systemstart gestartet werden soll
Description[en]=Defines if the DHCP service should be started automatically at system startup
Type=bool
Categories=service-dhcp,system-services

[dhcpd/restart/listener]
Description[de]=Definiert, ob der DHCP-Dienst automatisch durch den Listener neu gestartet wird
Description[en]=Defines if the DHCP service should be re-started automatically by the Listener
Type=bool
Categories=service-dhcp,system-services

[dhcpd/ddns.*]
Description[de]=Dynamic DNS Konfiguration
Description[en]=Dynamic DNS Settings
Type=str
Categories=service-dhcp

[dhcpd/options/.*]
Description[de]=DHCP Optionendefinitionen
Description[en]=DHCP option definitions
Type=str
