VERSION=8
. /usr/share/univention-join/joinscripthelper.lib
joinscript_init

. /usr/share/univention-lib/all.sh

eval "$(univention-config-registry shell)"

getent group lp | grep -q "$hostname" || adduser $hostname\$ lp || true


# Copy the previously fetched SSL certificates to the Cups directory:
if test -e "/etc/univention/ssl/$hostname.$domainname/cert.pem"; then
	rm -f /etc/cups/ssl/server.crt
	ln -s /etc/univention/ssl/$hostname.$domainname/cert.pem /etc/cups/ssl/server.crt
fi

if test -e "/etc/univention/ssl/$hostname.$domainname/private.key"; then
	rm -f /etc/cups/ssl/server.key
      ln -s /etc/univention/ssl/$hostname.$domainname/private.key /etc/cups/ssl/server.key
fi

# keep/delete old printer ppds
if [ $JS_LAST_EXECUTED_VERSION -le 7 ]; then
	if [ "$cups_keep_ppds" != "true" ]; then
		backupfile=/var/backups/ldap-hp-ppds-`date +"%d-%m-%y-%H-%M-%S"`
		univention-ldapsearch 'objectclass=univentionPrinterModels' >  $backupfile
		IFS=":"
		for i in $(udm settings/printermodel list "$@" | grep ^DN:| sed 's|^DN: |:|'); do
			if [ -n "$i" ]; then
				univention-directory-manager settings/printermodel remove "$@" --dn "$i"
			fi
		done
		unset IFS
	fi
fi

univention-directory-manager container/cn create "$@" --ignore_exists --position "cn=univention,$ldap_base" --set name=cups || die

univention-directory-manager settings/printermodel create "$@" --ignore_exists \
	--position "cn=cups,cn=univention,$ldap_base" \
	--set name=misc  \
	--append printmodel="\"None\" \"None\"" \
	--append printmodel="\"smb\" \"smb\"" || die

